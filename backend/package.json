{
  "name": "plinko-backend",
  "version": "1.0.0",
  "description": "Plinko game backend server with provably fair gaming",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon --watch . --ext js --exec node server.js",
    "test": "jest --watchAll=false",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "build": "prisma generate && prisma db push --skip-generate",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio"
  },
  "keywords": [
    "plinko",
    "game",
    "provably-fair",
    "express",
    "prisma"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=18 <23",
    "npm": ">=9"
  },
  "dependencies": {
    "@prisma/client": "^5.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "prisma": "^5.6.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1"
  }
}